<h1>Editing Book</h1>
<%= form_with model:@book,url:"/books/236411/edit",local:true do |f| %>
 <h4>Title</h4>
 <%= f.text_field :title %>
 <h4>Body</h4>
 <%= f.text_area :body %>
 <%= f.submit 'Update Book' %>
 
 <%= link_to 'Show',book_path(@book) %>
 <%= link_to 'Edit',edit_book_path %>
<% end %>
